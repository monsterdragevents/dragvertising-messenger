# Supabase Configuration
# Copy this file to .env.local and fill in your actual values
# These should match your main Dragvertising app's Supabase project

VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Main Dragvertising App URL
# This messenger is a child app of the main DragvertisingApp
# Set this to your main app's URL (e.g., https://dragvertising.app or http://localhost:3000 for local dev)
VITE_MAIN_APP_URL=https://dragvertising.app

# Optional: Development mode
# Set to 'true' to enable development features
VITE_DEV_MODE=false
